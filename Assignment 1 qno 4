#include<stdio.h>

int main(){
    int i, j, rows = 5;

    // Loop for rows
    for (i = 1; i <= rows; i++) {
        // Loop for printing stars
        for (j = 1; j <= i; j++) {
            printf("*");
        }
        // Move to the next line after printing each row
        printf("\n");
    }

    return 0;
}
